{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/**.json",
          "api/index.md"
        ]
      },
      {
        "files": [
          "vmchooser/**.md",
          "vmchooser/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "diskchooser/**.md",
          "diskchooser/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "bulkuploader/**.md",
          "bulkuploader/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "vmoptimizer/**.md",
          "vmoptimizer/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "port": "8000",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}